<h1>Edit student <%= @student.id %></h1>


<%= form_with model: @student do |form| %>
<div class="form-group">
<%= form.label :name %>
<%= form.text_field :name , class:'form-control'  %>
</div>
<div class="form-group">
<%= form.label :age %>
<%= form.text_field :age , class:'form-control' %>
</div>
<div class="form-group">
<%= form.label :email %>
<%= form.email_field :email , class:'form-control' %>
<div class="form-group">

<%= form.submit class:'btn btn-secondary btn-block mt-3'%>

<% end %>
